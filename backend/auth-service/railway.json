{
  "$schema": "https://railway.app/railway-schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "yarn install && yarn workspace @flexbox/auth-service build"
  },
  "deploy": {
    "startCommand": "node backend/auth-service/dist/index.js",
    "restartPolicyType": "ON_FAILURE"
  }
}
